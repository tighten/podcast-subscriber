{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "botman/botman": "^2.1",
        "botman/driver-facebook": "^1.5",
        "botman/driver-web": "^1.3",
        "botman/studio-addons": "^1.1",
        "botman/tinker": "~1.0",
        "bugsnag/bugsnag-laravel": "^2.0",
        "fideloper/proxy": "~3.3",
        "laravel-frontend-presets/tailwindcss": "^0.4.1",
        "laravel-notification-channels/facebook": "^0.0.1",
        "laravel-notification-channels/twilio": "^2.0",
        "laravel-notification-channels/twitter": "^1.0",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "propaganistas/laravel-phone": "^3.0",
        "willvincent/feeds": "dev-master"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "BotMan\\Studio\\Providers\\DriverServiceProvider::publishDriverConfigurations",
            "@php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "BotMan\\Studio\\Providers\\DriverServiceProvider::publishDriverConfigurations",
            "@php artisan optimize"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
